{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "nodenext",
    "outDir": "./dist",
    "declaration": true,
    "incremental": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "strictPropertyInitialization": true,
  }
}
